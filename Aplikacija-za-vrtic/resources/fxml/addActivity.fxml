<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane hgap="10.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="10.0" minWidth="10.0" vgap="10.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ba.unsa.etf.rpr.AddActivityController">
    <rowConstraints>
        <RowConstraints maxHeight="1.7976931348623157E308" minHeight="10.0" percentHeight="0.0" />
        <RowConstraints maxHeight="1.7976931348623157E308" minHeight="10.0" percentHeight="0.0" />
        <RowConstraints maxHeight="1.7976931348623157E308" minHeight="10.0" percentHeight="0.0" />
      <RowConstraints />
      <RowConstraints />
    </rowConstraints>
    <columnConstraints>
        <ColumnConstraints maxWidth="1.7976931348623157E308" percentWidth="0.0" />
      <ColumnConstraints />
    </columnConstraints>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
    <children>
        <TextField fx:id="nameField" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="38.0" prefWidth="137.0" promptText="Name" GridPane.rowIndex="1">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </TextField>
        <Label alignment="CENTER" contentDisplay="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="10.0" minWidth="10.0" text="Activity information" textAlignment="CENTER" GridPane.columnSpan="2">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </Label>
        <ButtonBar maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" GridPane.rowIndex="4">
            <buttons>
                <Button fx:id="buttonOk" alignment="CENTER" contentDisplay="CENTER" defaultButton="true" mnemonicParsing="false" onAction="#actionOk" text="OK" textAlignment="CENTER" />
                <Button fx:id="buttonCancel" cancelButton="true" mnemonicParsing="false" onAction="#actionCancel" text="Cancel" textAlignment="CENTER" />
            </buttons>
        </ButtonBar>
      <TextArea fx:id="descriptionArea" prefHeight="200.0" prefWidth="200.0" promptText="Description" GridPane.rowIndex="2" />
    </children>
</GridPane>
