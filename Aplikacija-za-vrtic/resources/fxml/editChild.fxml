        <buttons>
            <RadioButton fx:id="unratedRadio" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#actionUnrated" text="UNRATED">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding></RadioButton>
            <RadioButton fx:id="poorRadio" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#actionPoor" text="POOR">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding></RadioButton>
            <RadioButton fx:id="averageRadio" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#actionAverage" text="AVERAGE">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding></RadioButton>
            <RadioButton fx:id="goodRadio" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#actionGood" text="GOOD">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding></RadioButton>
            <RadioButton fx:id="verygoodRadio" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#actionVerygood" text="VERY GOOD">
               <padding>
                  <Insets bottom="10.0" top="10.0" />
               </padding></RadioButton>
            <RadioButton fx:id="excellentRadio" mnemonicParsing="false" onAction="#actionExcellent" text="EXCELLENT" />
        </buttons>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </ButtonBar>
      <ChoiceBox fx:id="activityChoice" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="5" />
      <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Current activity:" GridPane.rowIndex="5">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </Label>
    </children>
</GridPane>
