<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane hgap="10.0" vgap="10.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
    <rowConstraints>
        <RowConstraints maxHeight="1.7976931348623157E308" percentHeight="0.0" />
        <RowConstraints maxHeight="1.7976931348623157E308" percentHeight="0.0" />
        <RowConstraints maxHeight="1.7976931348623157E308" percentHeight="0.0" />
        <RowConstraints maxHeight="1.7976931348623157E308" percentHeight="0.0" />
        <RowConstraints maxHeight="1.7976931348623157E308" minHeight="45.60000000000001" percentHeight="0.0" prefHeight="45.60000000000001" />
        <RowConstraints maxHeight="1.7976931348623157E308" minHeight="10.0" percentHeight="0.0" prefHeight="30.0" />
